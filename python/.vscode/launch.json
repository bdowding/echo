    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Generate",
            "type": "debugpy",
            "request": "launch",
            "program": "rpc_lite/generate_api.py",
            "console": "integratedTerminal",
            "args": ["--input_yamls", "example/api.yaml", "--output_folder", "example/output"]
        },
        {
            "name": "echo",
            "type": "debugpy",
            "request": "launch",
            "program": "echo.py",
            "console": "integratedTerminal"
        }
    ]
}